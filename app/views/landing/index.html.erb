<header id='nav'>
  <%= link_to 'Home', '/' %>
</header>

<article>
  <h1>Viewing Party Light</h1>

  <%= button_to 'Create a New User', '/register', method: :get %>

  <div id="user-list">
    Existing Users:
    <ul>
    <% @users.each do |user| %>
      <li class="user-dash-link">
        <%= link_to "#{user.email}'s Dashboard", user_path(user) %>
      </li>
      <%end%>
    </ul>
  </div>
</article>